<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_resultoperate"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >

    <Button
        android:id="@+id/layout_resultoperate_save"
        style="@style/PageoperateButton"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:text="@string/save_history" 
        android:layout_marginTop="10dip"/>

    <Button
        android:id="@+id/layout_resultoperate_consulting"
        style="@style/PageoperateButton"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:text="@string/app_consultinline" 
        android:layout_marginTop="10dip"/>

</LinearLayout>