<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">
    <dubbo:application name="hello-world-app" />

    <dubbo:registry protocol="zookeeper" address="192.168.100.200:2181" />
<!--     <dubbo:registry address="redis://192.168.100.200:6379"/> -->
<!--     <dubbo:registry address="multicast://224.5.6.7:1234" />  -->

    <dubbo:protocol name="dubbo" port="20880" serialization="kryo"/>
<!--     <dubbo:protocol name="rmi" /> -->
<!--     <dubbo:protocol server="jetty" /> -->
<!--     <dubbo:protocol server="mina" /> -->
<!--     <dubbo:protocol server="grizzly" /> -->

    <bean id="userService" class="com.acme.dubbo.provider.UserServiceProvider" />
    <!-- 和本地bean一样实现服务 -->
    <dubbo:service interface="com.acme.dubbo.inter.UserService" ref="userService" />
</beans>